{
  "name": "simple-react-redux-ssr",
  "version": "1.0.0",
  "description": "Server side rendiring application of chareacters of Rick and Morty show in react and redux",
  "main": "index.js",
  "scripts": {
    "webpack:client": "webpack --config webpack.client.js --watch",
    "webpack:server": "webpack --config webpack.server.js --watch",
    "webpack:start": "wait-on build/public/client_bundle.js && nodemon --watch build --exec node build/bundle.js",
    "start": "rimraf ./build && npm-run-all --parallel webpack:*",
    "lint": "eslint src/client/**/*.js src/client/**/*.jsx"
  },
  "private": true,
  "author": "Yogesh DV <dvyogesh123@gmail.com>",
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@yoge-shource/grid-carousel-react": "^0.1.8",
    "express": "^4.14.0",
    "history": "^4.10.1",
    "loadash": "^1.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^7.0.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.1.0",
    "wait-on": "^5.0.0"
  },
  "devDependencies": {
